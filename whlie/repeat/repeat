// Playground - noun: a place where people can play

import UIKit

var index = 0
index = 0
while index<3{
    print("index is \(index)")
    index += 1
}

index = 0
repeat{
    print("index is \(index)")
    index += 1
} while index<3

index = 3
repeat{
    print("index is \(index)")
    index += 1
}while index<3


